<?xml version="1.0"?>
<yandex>

{% for item in clickhouse_docker_config %}
    <{{ item.keys()[0] }} replace="1">{{ item.values()[0] }}</{{ item.keys()[0] }}>
{% endfor %}

    <resharding replace="1">
{% for item in clickhouse_docker_config_resharding %}
        <task_queue_path replace="1">{{ item }}</task_queue_path>
{% endfor %}
    </resharding>

    <distributed_ddl replace="1">
{% for item in clickhouse_docker_config_distributed_ddl %}
       <path replace="1">{{ item }}</path>
{% endfor %}
    </distributed_ddl>

</yandex>
